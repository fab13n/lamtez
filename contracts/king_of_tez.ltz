@royal_key         :: key
@enthronement_date :: time
@enthronement_cost :: tez

\parameter :: key:
( case @enthronement_date + 604800 > self-now or self-amount > @enthronement_cost
| True: @royal_key <- parameter;
        @enthronement_date <- self-now;
        @enthronement_cost <- self-amount;
        ()
| False: fail
)
